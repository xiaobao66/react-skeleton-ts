{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "noFallthroughCasesInSwitch": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "noUnusedLocals": true,
    "noImplicitAny": true,
    "importsNotUsedAsValues": "preserve",
    "target": "es2018",
    "module": "esnext",
    "jsx": "react",
    "baseUrl": "./",
    "paths": {
      "pages/*": ["src/pages/*"],
      "models/*": ["src/models/*"],
      "store/*": ["src/store/*"],
      "utils/*": ["src/utils/*"]
    }
  },
  "exclude": [
    "node_modules"
  ]
}
